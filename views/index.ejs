<% include partials/header %>
<div class="container main">
    <h1>INDEX PAGE!!!!</h1>

    <% blogs.forEach((blog)=>{ %>
        <div>
            <h2><%= blog.title %></h2>
            <img src="<%= blog.image %>" >
            <p><%= blog.created %></p>
            <p><%= blog.body.substring(0,100) %>...</p>
            <a href="/blogs/<%= blog._id %>">Read More</a>
        </div>
    <% }) %>  
</div>

<% include partials/footer %>